{
	"name": "knax/online-market-backend",
	"description": "Online Market Backend",
	"keywords": ["market"],
	"license": "GPL3",
	"require": {
		"laravel/framework": "4.2.*",
		"barryvdh/laravel-cors": "0.2.x"
	},
	"require-dev": {
		"way/generators": "2.*",
		"phpunit/phpunit": "4.2.*",
		"fzaninotto/faker": "*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
